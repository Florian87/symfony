{% extends "TroiswaBackBundle::layout.html.twig" %}

{% form_theme formulaire _self %}

{% block form_errors -%}
    {% if errors|length > 0 -%}
        {%- for error in errors -%}
            <p>{{ error.message }}</p>
        {%- endfor -%}
    {%- endif %}
{%- endblock form_errors %}

{% block content %}

       <div class="row">
        <div class="col-lg-12">

				{{ form(formulaire) }}

        </div>
    </div>

{% endblock %}